
@{
    ViewBag.Title = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
    <br />
</div>

<div class="col-md-9 col-md-offset-3" >
    <div class="row">
        <div class="col-md-offset-2 col-md-3">
            <div class="form-login">
                <h4>Welcome back.</h4>
                <input type="text" id="userName" class="form-control input-sm chat-input" placeholder="Username" />
                </br>
                <input type="text" id="userPassword" class="form-control input-sm chat-input" placeholder="Password" />
                </br>
                <div class="wrapper">
                    <span class="group-btn">
                        <button class="btn btn-success btn-md">@Html.ActionLink("LOGIN", "Login", "Login", new object { }, new { @class = "buttonAttr" })</button>
                    </span>
                    <span class="group-btn">
                        <button class="btn btn-danger btn-md">@Html.ActionLink("CANCEL", "Index", "Home", new object { }, new { @class = "buttonAttr" })</button>
                    </span>
                </div>
            </div>

        </div>
    </div>
</div>    



